<h1>Listing items</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Due</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list.items.each do |item| %>
      <tr>
        <td><%= item.content %></td>
        <td><%= item.due %></td>
      </tr>
    <% end %>
  </tbody>
</table>

